<!DOCTYPE html>
<html lang="en">
  <!-- head -->
  <%- include(config.folders.layouts.path + '/partials/head'); -%>
  <body>
    <!-- main -->
    <div class="grid-wrapper">
      <% if (model.tags || !model.guid || !model.slug || model.pagination) { %>
      <header id="header">
        <a
          href="https://mbsy.co/3lfRTn"
          target="_blank"
          rel="noopener noreferrer"
        >
          <picture>
            <source
              media="(max-width: 799px)"
              srcset="/images/dreamhost-banner-sm.jpg" />
            <source
              media="(min-width: 800px)"
              srcset="/images/dreamhost-banner-lg.png" />
            <img
              src="/images/dreamhost-banner-lg.png"
              alt="DreamHost" /></picture
        ></a>
      </header>
      <% } %>
      <main id="main">
        <article>
          <!-- image -->
          <% if (model.image) { %>
          <img
            height="350"
            width="927"
            class="post-image"
            alt="<%= model.imageAlt %>"
            src="/images/<%= model.image %>"
          />
          <% } %>
          <!-- content -->
          <% if (model.title && model.guid) { %>
          <div
            class="article-content-wrapper <%= model.image ? 'has-image' : '' %>"
          >
            <% } %>

            <!-- title -->
            <% if (model.title) { %>
            <h1 class="title"><%= model.title %></h1>
            <% if (!model.guid) { %>
            <div class="content-banner">
              <div id="408475226">
                <script>
                  try {
                    window._mNHandle.queue.push(function () {
                      window._mNDetails.loadTag(
                        "408475226",
                        "728x90",
                        "408475226"
                      );
                    });
                  } catch (error) {}
                </script>
              </div>
            </div>
            <% } %>
            <!--  -->
            <% } %>
            <!--  -->
            <% if (model.title && model.guid && model.tags) { %>
            <!-- meta -->
            <%- include(config.folders.layouts.path + "/partials/postMeta") %>
            <!--  -->
          </div>

          <div class="content-banner">
            <div id="408475226">
              <script>
                try {
                  window._mNHandle.queue.push(function () {
                    window._mNDetails.loadTag(
                      "408475226",
                      "728x90",
                      "408475226"
                    );
                  });
                } catch (error) {}
              </script>
            </div>
          </div>

          <div class="article-content-wrapper">
            <!--  -->
            <% } %>
            <!--  -->
            <% if (model.layout.startsWith("archives/")) { %>
            <!--  -->
            <%- include(config.folders.layouts.path + '/' + model.layout) %>
            <!--  -->
            <% } else { %>
            <!--  -->
            <%- model.content %>
            <!--  -->
            <% if (model.path.startsWith("posts/")) { %>

            <p class="edit-post-line">
              <a
                href="https://github.com/jdedwards3/devextent/blob/main/content/<%= model.path %>"
                >Edit this post on GitHub</a
              >.
            </p>
            <% } %>
            <!--  -->
            <% } %>
            <!-- end content -->
            <% if (model.title && model.guid) { %>
          </div>
          <% } %>
        </article>
      </main>
      <aside id="aside">
        <%- include(config.folders.layouts.path + '/partials/sidebar'); -%>
        <!--  -->
        <div class="aside-banner-container">
          <% if (model.tags || !model.guid || !model.slug || model.pagination) {
          %>
          <a
            class="aside-banner"
            href="https://mondaycom.grsm.io/3104-pm"
            target="_blank"
            rel="noopener noreferrer"
          >
            <img src="/images/monday-square.png" alt="monday.com"
          /></a>
          <% } %>
        </div>
      </aside>
      <% if (model.tags || !model.guid || (model.guid && model.pagination &&
      (model.pagination.first || model.pagination.last)) ) { %>
      <aside id="after-main">
        <div class="inner">
          <script>
            amzn_assoc_tracking_id = "devextent06-20";
            amzn_assoc_ad_mode = "manual";
            amzn_assoc_ad_type = "smart";
            amzn_assoc_marketplace = "amazon";
            amzn_assoc_region = "US";
            amzn_assoc_design = "enhanced_links";
            amzn_assoc_asins = "B088BC5HMM";
            amzn_assoc_placement = "adunit";
            amzn_assoc_linkid = "bb45c084db290b8e22c28990318e142f";
          </script>
          <script src="//z-na.amazon-adsystem.com/widgets/onejs?MarketPlace=US"></script>
        </div>
      </aside>
      <% } %>
      <footer id="footer">
        <%- include(config.folders.layouts.path + '/partials/footer'); -%>
      </footer>
    </div>
    <script
      type="module"
      src="/scripts/<%= config.version %>/script.js"
    ></script>
  </body>
</html>
