<div id="sidebar">
  <h2 class="visually-hidden">Sidebar</h2>
  <div id="sidebar-inner">
    <% if (model.slug) { %>
    <nav>
      <a href="/">Home</a>
    </nav>
    <% } %>
    <form
      id="search-form"
      action="https://www.google.com/search"
      method="get"
      class="search"
    >
      <input
        type="hidden"
        name="q"
        id="q"
        value="site:<%= config.environment.domain %>"
      />
      <label for="search-str">Search</label>
      <input type="text" name="q" id="search-str" />
      <button type="submit" class="submit">Search</button>
    </form>
    <div class="ad-wrapper">
      <div id="408475226">
        <script type="text/javascript">
          try {
            window._mNHandle.queue.push(function () {
              window._mNDetails.loadTag("408475226", "728x90", "408475226");
            });
          } catch (error) {}
        </script>
      </div>
    </div>
    <!-- tags -->
    <% if (model.archives['tags']) { %>
    <div class="tag-list-container">
      <h3>All Tags:</h3>
      <ul class="tag-list">
        <% model.archives['tags'].forEach(function(tagData) { %>
        <li>
          <a href="/<%= tagData.slug %>/"><%= tagData.name %></a>
        </li>
        <% }); %>
      </ul>
    </div>
    <% } %>
  </div>
</div>
